$code-lang = {
  html:"HTML", js:"JavaScript", javascript: "JavaScript", ts:"TypeScript", typescript: 'TypeScript'
  css:"CSS", scss:"SCSS", diff:"Diff", java:"Java", xml:"XML",
  python:"Python", yml:"YAML", yaml:"YAML", swift:"Swift", ruby:"Ruby", perl:"Perl",
  php:"PHP", c:"CLang", cpp:"C++", cs:"C#", sh:"Shell", bash:"bash",
  toml: "TOML", rust: "Rust", sh: "Shell", go: "Go", json: "JSON", nft:"nftables",
}
for $s, $t in $code-lang
    .highlight.{$s}:after
        content: $t
