{% extends "_layout.njk" %}
{% import "_partial/archive.njk" as archivet with context %}

{% block title %}
  {% if page.archive %}
    {{ __('nav.archives') }} · {{ config.title }}
  {% elif page.tag %}
    {{ page.tag }} · {{ config.title }}
  {% endif %}
{% endblock %}

{% block main %}
    {{ archivet.render() }}
{% endblock %}
